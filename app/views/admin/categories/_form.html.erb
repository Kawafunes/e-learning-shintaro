<%= form_for(:category, url: admin_categories_path) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, class: "form-control" %>
  <br>
  <%= f.label :description %>
  <%= f.text_area :description, class: "form-control" %>
  <br>
  <div class="btn-2">
  <%= f.submit "Edit", class: "btn btn-primary" %>
  </div>
<% end %>